{
  "AppSettings": {
    "ConnectionString": "Data Source=(local);Initial Catalog=DataManagement;Integrated Security=True",
    "IdentityConnectionString": "Server=(localdb)\\mssqllocaldb;Database=DataManagement;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.AspNetCore.DataProtection": "Error"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "\\farm-manager\\logs\\logs.log",
          "rollOnFileSizeLimit": true,
          "rollingInterval": "Day"
        }
      }
    ],
    "Enrich": [
      "FromLogContext"
    ]
  },
  "AllowedHosts": "*"
}